<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="500.0" style="-fx-background-color: #708090;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MembershipController">
   <children>
      <TableView fx:id="membershipFeesTableView" layoutY="70.0" prefHeight="300.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="typeColumn" minWidth="125.0" prefWidth="125.0" text="Type" />
          <TableColumn fx:id="amountColumn" minWidth="125.0" prefWidth="125.0" text="Amount" />
            <TableColumn fx:id="isPaidColumn" minWidth="125.0" prefWidth="125.0" text="Paid" />
            <TableColumn fx:id="monthColumn" minWidth="125.0" prefWidth="125.0" text="Month" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="26.0" text="Find membership fee:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="findMembershipFeeField" layoutX="160.0" layoutY="22.0" prefHeight="30.0" prefWidth="310.0" />
      <Button fx:id="createMembershipFeeBtn" layoutX="72.0" layoutY="394.0" mnemonicParsing="false" onAction="#createMembershipFee" text="Create">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="logPaymantBtn" layoutX="265.0" layoutY="394.0" mnemonicParsing="false" onAction="#logPaymant" text="Log paymant">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
