<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="450.0" prefWidth="500.0" style="-fx-background-color: #708090;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="evidentiranjeTableView" layoutY="70.0" prefHeight="300.0" prefWidth="500.0">
         <columns>
            <TableColumn fx:id="datumColumn" minWidth="250.0" prefWidth="250.0" text="Datum dolazaka i odlazaka" />
            <TableColumn fx:id="vrijemeColumn" minWidth="250.0" prefWidth="250.0" text="Vrijeme dolazaka i odlazaka" />
         </columns>
      </TableView>
      <Button fx:id="evidentirajDolazakBtn" layoutX="51.0" layoutY="398.0" mnemonicParsing="false" onAction="#evidentirajVrijemeDolaska" text="Evidentiraj dolazak">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="evidentirajOdlazakBtn" layoutX="279.0" layoutY="398.0" mnemonicParsing="false" onAction="#evidentirajVrijemeOdlaska" text="Evidentiraj odlazak">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="26.0" text="Pronađite odgovarajući datum:">
         <font>
            <Font size="15.0" />
         </font></Label>
      <TextField fx:id="pronadjiOdgovarajuciDatumField" layoutX="226.0" layoutY="22.0" prefHeight="30.0" prefWidth="260.0" />
   </children>
</AnchorPane>
